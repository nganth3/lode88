{
  "name": "lode88-api",
  "private": true,
  "version": "1.0.0",
  "author": "ITME",
  "scripts": {
    "lint": "eslint src --ext js --report-unused-disable-directives --max-warnings 0",
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel ./src -d ./build/src --copy-files",
    "build-babel-api": "babel ./api -d ./build/api",
    "build": "npm run clean && npm run build-babel && npm run build-babel-api",
    "start": "node ./build/api/index.js ",
    "production": "npm run build && node ./build/src/server.js",
    "dev": "nodemon --exec babel-node ./src/server.js",
    "startws": "npx babel-node ./src/sockets/serverSocket.js",
    "test": "npx babel-node ./src/services/test.js"
  },
  "dependencies": {
    "@babel/runtime": "^7.22.10",
    "async-exit-hook": "^2.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "ejs": "^3.1.10",
    "express": "^5.1.0",
    "http-status-codes": "^2.3.0",
    "moment": "^2.30.1",
    "mongodb": "^6.16.0",
    "websocket": "^1.0.35"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.10",
    "@babel/core": "^7.22.10",
    "@babel/eslint-parser": "^7.22.10",
    "@babel/node": "^7.22.10",
    "@babel/plugin-transform-runtime": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "@html-eslint/eslint-plugin": "^0.40.3",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.47.0",
    "nodemon": "^3.1.10"
  }
}
